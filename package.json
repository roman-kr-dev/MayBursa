{
  "name": "turborepo-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Turborepo monorepo with OptionsSniper and Intuitions projects",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:optionssniper": "turbo run dev --filter=@monorepo/optionssniper*",
    "dev:intuitions": "turbo run dev --filter=@monorepo/intuitions*",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.4.5",
    "prettier": "^3.3.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}